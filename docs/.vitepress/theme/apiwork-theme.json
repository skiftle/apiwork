{
  "name": "apiwork",
  "type": "dark",
  "colors": {
    "editor.background": "#1a1214",
    "editor.foreground": "#e4e4e7"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6b7280",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "string.template",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#86efac"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#fdba74"
      }
    },
    {
      "name": "Boolean/Nil",
      "scope": ["constant.language.boolean", "constant.language.nil", "constant.language.null", "constant.language.undefined"],
      "settings": {
        "foreground": "#fdba74"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.logical",
        "keyword.operator.new",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#a78bfa"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator", "keyword.operator.assignment"],
      "settings": {
        "foreground": "#a78bfa"
      }
    },
    {
      "name": "Function definition",
      "scope": [
        "entity.name.function",
        "meta.function entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#c4b5fd"
      }
    },
    {
      "name": "Function call",
      "scope": ["meta.function-call", "support.function.kernel.ruby"],
      "settings": {
        "foreground": "#60a5fa"
      }
    },
    {
      "name": "Class",
      "scope": [
        "entity.name.class",
        "entity.name.type.class",
        "support.class",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#fbbf24"
      }
    },
    {
      "name": "Module",
      "scope": ["entity.name.module", "entity.name.namespace"],
      "settings": {
        "foreground": "#fbbf24"
      }
    },
    {
      "name": "Type",
      "scope": [
        "entity.name.type",
        "support.type",
        "support.type.primitive"
      ],
      "settings": {
        "foreground": "#5eead4"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant.language",
        "support.constant",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#5eead4"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#e4e4e7"
      }
    },
    {
      "name": "Parameter",
      "scope": [
        "variable.parameter",
        "meta.function.parameters variable",
        "variable.parameter.function"
      ],
      "settings": {
        "foreground": "#fcd34d"
      }
    },
    {
      "name": "Instance variable",
      "scope": ["variable.other.readwrite.instance"],
      "settings": {
        "foreground": "#7dd3fc"
      }
    },
    {
      "name": "Property/Attribute",
      "scope": [
        "entity.other.attribute-name",
        "meta.object-literal.key",
        "support.type.property-name",
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#fcd34d"
      }
    },
    {
      "name": "Symbol",
      "scope": ["constant.other.symbol", "string.other.symbol"],
      "settings": {
        "foreground": "#5eead4"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "meta.brace",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#9ca3af"
      }
    },
    {
      "name": "Tag (HTML/XML)",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#a78bfa"
      }
    },
    {
      "name": "CSS Property",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#60a5fa"
      }
    },
    {
      "name": "CSS Value",
      "scope": ["support.constant.property-value.css"],
      "settings": {
        "foreground": "#fcd34d"
      }
    },
    {
      "name": "Regex",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#a78bfa"
      }
    },
    {
      "name": "Interpolation",
      "scope": [
        "punctuation.section.embedded",
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#a78bfa"
      }
    },
    {
      "name": "JSON Key",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#fcd34d"
      }
    },
    {
      "name": "YAML Key",
      "scope": ["entity.name.tag.yaml"],
      "settings": {
        "foreground": "#fcd34d"
      }
    },
    {
      "name": "Markdown Heading",
      "scope": ["markup.heading", "entity.name.section.markdown"],
      "settings": {
        "foreground": "#fbbf24",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#60a5fa"
      }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#86efac"
      }
    }
  ]
}
